@model IEnumerable<SQLDependancySignalR.JobInfo>


@{
    ViewBag.Title = "About";
}


@foreach(var a in Model)
{
    @Html.DisplayFor(modelItem=>a.JobID)
    @Html.DisplayFor(modelItem => a.Name)

    @Html.DisplayFor(modelItem => a.Status)
    @Html.DisplayFor(modelItem => a.LastExecutionDate)
    <br />
}

